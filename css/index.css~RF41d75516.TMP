/* Default Style sheet - Header & Footer & navigation
-------------------------------------------------*/
@import url('https://cdn.rawgit.com/theeluwin/NotoSansKR-Hestia/master/stylesheets/NotoSansKR-Hestia.css');
@import url('fonts/stylesheet/NotoSansKR-Hestia.css');
@font-face{
	font-family: 'Noto Sans Korean', sans-serif;}
	/*
	src:url('../fonts/NotoSans.eot');
	src:url('../fonts/NotoSans.eot?#iefix') format(‘embedded-opentype’),
	url('../fonts/NotoSans.woff') format(‘woff’),
	url('../fonts/NotoSans.ttf') format('truetype')
}
*/
*{ margin:0; padding:0; border:0;}
html{ width: 100%; height:auto;}
body{ font-family: 'Noto Sans Korean', sans-serif; font-size:16px;/*1em*/ font-weight: 400; letter-spacing:-1px; width:100%;}
button{ letter-spacing: -1px;}
p{ font-size: 16px; display: inline;}
li{ list-style: none;}
a{ text-decoration:none; color:inherit; cursor: pointer;}
h1,h2,h3,h4,h5,h6{ display: block;}
h1{ font-size:43px;}
h2{ font-size:28px;}
h3{ font-size:26px;}
h4{ font-size:24px;}
h5{ font-size:22px;}
/* Header
-----------------------------------------------*/
#header{ height:137px;/*408px*/; border-top:4px solid #fe6b01;}
.center{ width:1140px;/*1140px*/ margin:0 auto; padding:0 10px;}
#main_tnb{ background-color: #f4f5f6; margin:0 auto;}
.title{ height:45px; line-height:45px; font-size:14px; color:#999;}
.title > strong{ color:#555;}
#header ul li{ float:left;}
.tnb_wrap{ height:45px; float:right;}
.tnb_area1{ height:30px; padding-top:14px; display: inline-block!important; margin-right:40px; vertical-align: top;}
.tnb_area2{ height:34px; display: inline-block!important; padding-top:10px;}
.tnb_area1 li a{ margin-left:16px!important; width:10px; height:15px;}
.tnb_area2 li{ height:29px; margin-left:5px;}
.tnb_area2 li a{ height:20px; padding:2px 10px; line-height:21px; border:1px solid #dedede; font-size:13px; border-radius: 3px; color:#666; background-color: #fff; font-weight: 600;}
/* Gnb - global navigation bar
-------------------------------------------*/
#main_gnb{ height:91px; border-bottom:1px solid #f4f4f6;}
#main_gnb .center{ position: relative;}
#main_gnb h1{ float:left; display: inline-block; margin-top:18px; width:155px; height:66px; font-size:50px; background: url('../images/logo.png') top left no-repeat; cursor: pointer;}
.main-logo{ width:155px; height:42px; padding-top:15px;}
.main-logo img{ margin-top:15px;}
.gnb_wrap{ height:92px; position: absolute; top:0; right:10px;}
.gnb_wrap li{ float:left; margin-left:94px; height:45px;/*92px*/; padding-top:47px;}
.gnb_wrap li:first-child{ margin-right:2px;}
.gnb_wrap li a{ font-size:18px; font-weight:700; padding-bottom: 17px; height:65px;}
.gnb_wrap li a:hover{ border-bottom: 4px solid #fe6b01;}
#gnb_sub{ display: none; width:100%; height:266px; border-bottom: 1px solid #a3a3a3; position:absolute; top:141px; z-index: 999; background: #fff;}
#gnb_sub ul{ float:right; padding-top:10px;}
#gnb_sub ul li{ float:left; margin-left:87px; text-align: center; display: inline-block;}
#gnb_sub ul li:nth-child(5){ margin-right:5px;}
#gnb_sub ul li:nth-child(4){ margin-right:20px;}
#gnb_sub ul li:nth-child(3){ margin-right:-8px;}
#gnb_sub ul li:nth-child(2){ margin-right:-8px;}
#gnb_sub ul li:nth-child(1){ margin-right:-12px;}
#gnb_sub a{ display: block; font-size: 15px; line-height:38px; color:#666; font-weight:500;}
#gnb_sub a:hover{ font-weight:bold; color:#222;}
/* LNB - Low navigation
-------------------------------------------*/
nav{ height:291px!important;}
.htitle_panel{ height:230px; background: url('../images/bg_sub_title.jpg') no-repeat;}
.htitle_panel h1{ color:#222; font-size:50px; text-align:center; padding-top:55px; color:#fff; font-weight:600;}
.htitle_panel h2{ color:#222; font-size:24px; text-align:center; padding-bottom:73px; color:#fff; margin-top:5px; font-weight:400;}
.lnb{ width:100%; height:50px; border-bottom:1px solid #dedede;}
.lnb .gohome{ background: url('../images/btn_home.jpg'); width:51px; height:51px; display: inline-block; float:left;}
.lnb .right_area{ float:right;}
.gohome{ float:left; display: inline;}
.dropdown p{ width:100%; background: #fff; color:#333; cursor: pointer; display: inline-block; float:left; cursor: pointer;}
.dropdown { position:relative; display: inline-block; width:130px; padding:15px 12px; border-right:1px solid #ddd;}
.dropdown span{ display: inline-block; float:right; color:#555; margin-top:-3px;}
.dropdown ul{ display: none; position:absolute; z-index: 4; width:98%; top:51px; background: #fff; border: 1px solid #666; left:0; margin:0; color:#666;}
.dropdown ul li a{ display: block; background: #fff; padding-left:12px; line-height: 32px; font-size:0.937em;/*15px*/ color:#555;;}
.dropdown ul li a:hover{ background: #eee;}
.left_area{ float:left; height:51px;}
.right_area{ float:right; width:190px; height:51px; border-left:1px solid #ddd; border-right:1px solid #ddd; font-size:14px; color:#333; line-height: 51px;}
.right_area>img{ float:left; margin:20px 3px 0 12px;}
/* Aside Quick Banner
-------------------------------------------*/
.aside{ position:fixed; top:55%; left:89%; z-index: 10;}
.aside ul{ width:80px; height:120px; border: none; text-align: center; position: absolute; top:120px; left:30px;}
.aside ul li a{ padding:3px 0; color:#444; font-size: 14px; height:12px;}
/* Footer
------------------------------------------- */
.footer_wrap{ background-color: #fafafa; border-top:1px solid #eee;}
.area_1{ float:left;}
.footer_wrap .b_logo{ display: inline-block; padding-top:50px; float:left;}
.footer_wrap .adderess_wrap { float:left;  padding:42px 0 0 50px; height:146px;}
.ad_ul > li{ float:left; font-size:0.937em;/*15px*/ color:#eee;}
.ad_ul > li > a{ margin:0 8px; font-size:0.937em;/*15px*/ color:#666; font-weight:500;}
.ad_ul li:first-child > a{ margin-left: 0px;}
.address{ font-style: normal; font-size:13px; color:#999; font-weight: normal; margin-top:20px;}
.sign{ font-size:13px; color:#ddd; font-weight:300; padding-top:9px;}
.area_2{ height:188px; float:right;}
.footer_area1{ padding-top: 45px; float: left;}
.footer_area1 li{ float:left; margin-left:31px;}
.footer_area1 li a img{ filter: grayscale(90%); -webkit-filter: grayscale(90%);/*Safari 6.0-9.0*/ height:15px;}
.footer_area2{ float:left; width:242px; margin:38px 0 0 36px; font-weight: normal;}
footer #dropdown{ position: relative; display: inline-block;}
footer #dropdown p{ width:218px; height:24px;/*37px;*/ border:1px solid #ddd; padding: 8px 12px; float:right; display: inline-block; color;#ddd; font-size:14px; color:#9c9c9c; cursor: pointer;}
footer #dropdown ul{ min-width:240px; background: #fff; border: 1px solid #c8c8c8; position:absolute; top:-80px; overflow:auto; z-index:1; display: none;}
footer #dropdown ul li a{ padding:0 5px 0 12px; color:#666; font-size: 0.812em;/*13*/ line-height: 30px; text-align: left; display:block; cursor: pointer; font-size:14px;}
footer #dropdown ul li a:hover{ font-weight: bolder;}
#dropdown span{ float:right; font-size:20px;}
.footer_banner > img{ height: 76px; padding-top:5px; display: inline-block; margin-top:5px}
